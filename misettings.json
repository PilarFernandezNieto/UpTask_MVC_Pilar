{
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "workbench.colorTheme": "Flatland Monokai",
    "workbench.preferredDarkColorTheme": "Monokai",
    "eslint.enable": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ],
        "comments": "#bccdd2"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false,
    "workbench.settings.applyToAllProfiles": [],
    "editor.wordWrap": "on",
    "editor.rulers": [],
    "settingsSync.ignoredExtensions": [],
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "intelephense.format.braces": "k&r",
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "tailwindCSS.includeLanguages": {
        "html": "html",
        "vue": "vue",
        "javascript": "javascript",
        "css": "css"
    },
    "editor.wordWrapColumn": 100,
    "diffEditor.wordWrap": "on",
    "editor.formatOnPaste": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "git.openRepositoryInParentFolders": "never",
    "php.validate.executablePath": "C:/php/php.exe",
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "intelephense.diagnostics.undefinedClassConstants": false,
    "intelephense.diagnostics.undefinedConstants": false,
    "intelephense.diagnostics.undefinedFunctions": false,
    "intelephense.diagnostics.undefinedVariables": true,
    "intelephense.diagnostics.deprecated": false,
    "intelephense.diagnostics.undefinedProperties": false,
    "workbench.editor.enablePreview": false,
    "intelephense.diagnostics.undefinedTypes": false,
    "editor.fontLigatures": false,
    "terminal.integrated.fontFamily": "'Cascadia Code', 'Courier New', monospace",
    "git.terminalGitEditor": true,
    "editor.fontFamily": "Cascadia Code,'Courier New', monospace",
    "workbench.colorCustomizations": {},
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "intelephense.diagnostics.undefinedMethods": false,
    "intelephense.diagnostics.implementationErrors": false,
    "editor.linkedEditing": true,
    "php.linkedEditing.variables": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "javascript.suggest.completeFunctionCalls": true,
    "typescript.suggest.completeFunctionCalls": true,
    "intelephense.diagnostics.typeErrors": false
}